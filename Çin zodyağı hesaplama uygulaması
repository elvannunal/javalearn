import java.util.Scanner;

public class çinZodyağıHesaplama {
    public static void main(String[] args) {

        int dogumYiliniz;

        Scanner input=new Scanner(System.in);
        System.out.print("Doğum yılınızı giriniz: ");
        dogumYiliniz=input.nextInt();
         if(dogumYiliniz % 12==0){
            System.out.print("Çin zodyağı burcunuz: Maymun");
        }else if(dogumYiliniz % 12==1){
            System.out.println("Çin zodyağı burcunuz: Horoz");
        }else if(dogumYiliniz % 12==2){
            System.out.println("Çin zodyağı burcunuz: Köpek");
        }else if(dogumYiliniz % 12==3){
            System.out.println("Çin zodyağı burcunuz: Domuz");
        }else if(dogumYiliniz % 12==4){
            System.out.println(" Çin zodyağı burcunuz:Fare");
        }else if(dogumYiliniz % 12==5){
            System.out.println(" Çin zodyağı burcunuz: Öküz");
        }else if(dogumYiliniz % 12==6){
            System.out.println("Çin zodyağı burcunuz: Kaplan");
        }else if(dogumYiliniz % 12==7){
            System.out.println("Çin zodyağı burcunuz: Tavşan");
        }else if(dogumYiliniz % 12==8){
            System.out.println("Çin zodyağı burcunuz: Ejderha");
        }else if(dogumYiliniz % 12==9){
            System.out.println("Çin zodyağı burcunuz: Yılan");
        }else if(dogumYiliniz % 12==10){
            System.out.println("Çin zodyağı burcunuz: At");
        }else if(dogumYiliniz % 12==11){
            System.out.println("Çin zodyağı burcunuz: Koyun");
        }
    }
}
